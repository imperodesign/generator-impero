{
  "parser": "babel-eslint",
  "extends": ["standard"<% if (jsLang === 'react') { %>, "standard-react"<% } %>],
  "env": {
    "browser": true,
    "es6": true
  },
  "ecmaVersion": 2015,
  "sourceType": "module",
  "globals": {
    "DEVMODE": false
  }<% if (jsLang === 'react') { %>,
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "jsx-quotes": [2, "prefer-double"]
  }<% } %>
}
